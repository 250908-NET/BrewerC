{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "SchoolDemoSecretKeyYesItNeedsToBeThisLong",
    "Issuer": "SchoolDemo.Api",
    "Audience": "SchoolDemoUsers.Client",
    "ExpirationMinutes": 5
  }
}